#!/usr/bin/octave-cli -q

% Test with M16-6H

thr.P = 2;
thr.D = 16;
thr.TD2 = 212/1000;
thr.EID = thr.EID2 = thr.EID1 = 0;

g = mgage(thr, "plug");
print_gage(g, thr.P, thr.D, "plug");



% Test with M16-6g

clear thr g
thr.P = 2;
thr.d = 16;
thr.Td2 = 160/1000;
thr.esd = thr.esd2 = thr.esd1 = -38/1000;

g = mgage(thr, "ring");
print_gage(g, thr.P, thr.d, "ring");

clear g;
g = mgage(thr, "cplug");
print_gage(g, thr.P, thr.d, "cplug");

