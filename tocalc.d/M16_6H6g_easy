% Test with M16-6H

easy_mgage(p=2, d=16, ...
	d_vec=16+[0 inf], ...
	d2_vec=14.701+[0 0.212], ...
	d1_vec=13.835+[0 0.375], "plug");


% Test with M16-6g

easy_mgage(p=2, d=16, ...
	d_vec=16+[-0.038 -0.318], ...
	d2_vec=14.701+[-0.038 -0.198], ...
	d1_vec=13.835+[-0.038 -inf], "ring");

easy_mgage(p=2, d=16, ...
	d_vec=16+[-0.038 -0.318], ...
	d2_vec=14.701+[-0.038 -0.198], ...
	d1_vec=13.835+[-0.038 -inf], "cplug");

